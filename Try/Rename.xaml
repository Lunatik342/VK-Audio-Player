<Window x:Class="Try.Rename"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Try"
        mc:Ignorable="d"
        Title="Rename" Height="130" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Height="27"  TextWrapping="Wrap" VerticalAlignment="Top" Width="272" Margin="10,10,0,0"/>
        <Button x:Name="CancelButton" Click="CancelButton_OnClick" Content="Cansel" IsCancel="True" HorizontalAlignment="Left" Margin="127,56,0,0" VerticalAlignment="Top" Width="75" Height="25"></Button>
        <Button x:Name="ConfurmButton" Click="ConfurmButton_OnClick" Content="Ok" IsDefault="True" HorizontalAlignment="Left" Margin="207,56,0,0" VerticalAlignment="Top" Width="75" Height="25"/>

    </Grid>
</Window>
